{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended", "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
    },
    "overrides": [
      {
        "files": ["src/**/*.js,jsx"]
      }
    ]
}
